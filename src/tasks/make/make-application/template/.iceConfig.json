{
	"v2": {
		"name": "<%= dashName %>",
		"sassPath": "src/scss",
		"modulePath": "src/js",
		"watchPaths": [
			"src/**/*"
		],
		"excludePath": [],
		"noopPaths": [],
		"entries": [{
			"name": "Main",
			"entry": "index.jsx",
			"exit": "main.bundle"
		}],
		"outputPath": "bundles",
		"dev": {
			"path": "/bundles/",
			"contentBase": "./public",
			"port": 3000,
			"proxy": {}
		},
		"alias": {},
		"tests": {
			"watchPaths": [
				"src/**/*"
			],
			"coverageCollection": false
		},
		"translations": {
			"regexp": "\\bt\\b\\((?:'|\"|`)(.[^\\']*)(?:'|\"|`)\\s?(?:\\)|{|,)",
			"file": "config/i18n-expressions.json"
		},
		"plugins": {},
		"make": {
			"prefix": ""
		},
		"aliases": {}
	}
}
