{
	"v2": {
		"name": "<%= name %>",
		"sassPath": "src/scss",
		"modulePath": "src/js",
		"watchPaths": [
			"src/**/*",
		],
		"excludePath": [],
		"noopPaths": [],
		"entries": [{
			"name": "Main",
			"entry": "<%= entry %>",
			"exit": "[name].bundle"
		}],
		"outputPath": "bundles",
		"dev": {
			"path": "bundles",
			"proxy": {}
		},
		"alias": {},
		"tests": {
			"watchPaths": [
				"src/**/*"
			],
			"coverageCollection": [],
			"minimumCoverage": {
				"branches": 90,
				"functions": 100,
				"lines": 95,
				"statements": 90
			}
		},
		"translations": {
			"regexp": "",
			"file": ""
		},
		"plugins": {},
		"make": {
			"prefix": ""
		}
	}
}
