{
	"v2": {
		"name": "<%= name %>",
		"sassPath": "src/main/plugins/<%= plugin %>/ui.html/sources/scss",
		"modulePath": "src/main/plugins/<%= plugin %>/ui.html/sources/js",
		"copyPath": "src/main/plugins/<%= plugin %>",
		"targetPath": "target/classes/GLIDE-INF/plugins/<%= plugin %>",
		"watchPaths": [
			"src/main/plugins/<%= plugin %>/**/*",
			"src/main/java/**/*.java",
			"!src/main/plugins/<%= plugin %>/update/**/*",
			"!src/main/plugins/<%= plugin %>/upload/**/*",
			"!src/main/plugins/<%= plugin %>/apply_once/**/*"
		],
		"testWatchPaths": [
			"src/main/plugins/<%= plugin %>/ui.html/sources/js/**/*"
		],
		"coverageCollection": [],
		"minimumCoverage": {
			"branches": 90,
			"functions": 100,
			"lines": 95,
			"statements": 90
		},
		"entries": [{
			"name": "Main",
			"entry": "<%= entry %>",
			"exit": "[name].bundle"
		}],
		"outputPath": "src/main/plugins/<%= plugin %>/ui.html/bundles",
		"devPath": "/scripts/bundles/"
	}
}
